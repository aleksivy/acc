			<query>ВЫБРАТЬ
	Рег.Субконто2 КАК Договор,
	Рег.Субконто3 КАК ДокументВзаиморасчетов,
	СУММА(Рег.СуммаОстатокДт) КАК СуммаОстатокДт
ПОМЕСТИТЬ Остатки
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Остатки(НАЧАЛОПЕРИОДА(&amp;ДатаОстатков, ДЕНЬ), Счет = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.НалоговыйКредитНеподтвержденный), , ) КАК Рег

СГРУППИРОВАТЬ ПО
	Рег.Субконто2,
	Рег.Субконто3
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Рег.СубконтоДт2 КАК Договор,
	Рег.СубконтоДт3 КАК ДокументВзаиморасчетов,
	Рег.Период,
	Рег.Регистратор,
	СУММА(Рег.СуммаОборот) КАК СуммаОборот
ПОМЕСТИТЬ Обороты1
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ОборотыДтКт(
			ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(&amp;ДатаОстатков, ДЕНЬ), МЕСЯЦ, -6),
			НАЧАЛОПЕРИОДА(&amp;ДатаОстатков, ДЕНЬ),
			Регистратор,
			СчетДт = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.НалоговыйКредитНеподтвержденный),
			,
			СчетКт = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.РасчетыСОтечественнымиПоставщиками)
				ИЛИ СчетКт = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.НалоговыйКредит),
			,
			СубконтоДт2 В
					(ВЫБРАТЬ
						Остатки.Договор
					ИЗ
						Остатки КАК Остатки)
				И (СубконтоДт3 В
						(ВЫБРАТЬ
							Остатки.ДокументВзаиморасчетов
						ИЗ
							Остатки КАК Остатки)
					ИЛИ СубконтоДт3 = НЕОПРЕДЕЛЕНО
					ИЛИ СубконтоДт3 = NULL)) КАК Рег

СГРУППИРОВАТЬ ПО
	Рег.СубконтоДт2,
	Рег.СубконтоДт3,
	Рег.Период,
	Рег.Регистратор
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Рег.СубконтоКт2 КАК Договор,
	Рег.СубконтоКт3 КАК ДокументВзаиморасчетов,
	Рег.Регистратор.ДокументОснование КАК Регистратор,
	СУММА(Рег.СуммаОборот) КАК СуммаОборот
ПОМЕСТИТЬ ВходящиеНН
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ОборотыДтКт(
			ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(&amp;ДатаОстатков, ДЕНЬ), МЕСЯЦ, -6),
			НАЧАЛОПЕРИОДА(&amp;ДатаОстатков, ДЕНЬ),
			Регистратор,
			СчетДт = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.РасчетыПоНДС),
			,
			СчетКт = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.НалоговыйКредитНеподтвержденный),
			,
			СубконтоКт2 В
					(ВЫБРАТЬ
						Остатки.Договор
					ИЗ
						Остатки КАК Остатки)
				И (СубконтоКт3 В
						(ВЫБРАТЬ
							Остатки.ДокументВзаиморасчетов
						ИЗ
							Остатки КАК Остатки)
					ИЛИ СубконтоДт3 = НЕОПРЕДЕЛЕНО
					ИЛИ СубконтоДт3 = NULL)) КАК Рег
ГДЕ
	ТИПЗНАЧЕНИЯ(Рег.Регистратор) = ТИП(Документ.РегистрацияВходящегоНалоговогоДокумента)
	И НЕ Рег.Регистратор.ДокументОснование = НЕОПРЕДЕЛЕНО

СГРУППИРОВАТЬ ПО
	Рег.СубконтоКт2,
	Рег.СубконтоКт3,
	Рег.Регистратор.ДокументОснование
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Обороты1.Договор,
	Обороты1.ДокументВзаиморасчетов,
	Обороты1.Период,
	Обороты1.Регистратор,
	СУММА(Обороты1.СуммаОборот - ЕСТЬNULL(ВходящиеНН.СуммаОборот, 0)) КАК СуммаОборот
ПОМЕСТИТЬ Обороты
ИЗ
	Обороты1 КАК Обороты1
		ЛЕВОЕ СОЕДИНЕНИЕ ВходящиеНН КАК ВходящиеНН
		ПО Обороты1.Договор = ВходящиеНН.Договор
			И Обороты1.ДокументВзаиморасчетов = ВходящиеНН.ДокументВзаиморасчетов
			И Обороты1.Регистратор = ВходящиеНН.Регистратор

СГРУППИРОВАТЬ ПО
	Обороты1.Договор,
	Обороты1.ДокументВзаиморасчетов,
	Обороты1.Период,
	Обороты1.Регистратор

ИМЕЮЩИЕ
	СУММА(Обороты1.СуммаОборот - ЕСТЬNULL(ВходящиеНН.СуммаОборот, 0)) &gt; 0
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Обороты.Договор КАК Договор,
	Обороты.ДокументВзаиморасчетов КАК ДокументВзаиморасчетов,
	Обороты.Период КАК Период,
	МАКСИМУМ(Обороты.Регистратор) КАК Регистратор,
	СУММА(Обороты1.СуммаОборот) КАК НакопительнаяСумма
ПОМЕСТИТЬ НакопительныеСуммы1
ИЗ
	Обороты КАК Обороты
		ЛЕВОЕ СОЕДИНЕНИЕ Обороты КАК Обороты1
		ПО Обороты.Договор = Обороты1.Договор
			И Обороты.ДокументВзаиморасчетов = Обороты1.ДокументВзаиморасчетов
			И Обороты.Период &lt;= Обороты1.Период

СГРУППИРОВАТЬ ПО
	Обороты.Договор,
	Обороты.ДокументВзаиморасчетов,
	Обороты.Период
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	НакопительныеСуммы1.Договор,
	НакопительныеСуммы1.ДокументВзаиморасчетов,
	НакопительныеСуммы1.Период,
	НакопительныеСуммы1.Регистратор,
	НакопительныеСуммы1.НакопительнаяСумма,
	Обороты.СуммаОборот
ПОМЕСТИТЬ НакопительныеСуммы2
ИЗ
	НакопительныеСуммы1 КАК НакопительныеСуммы1
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Обороты КАК Обороты
		ПО НакопительныеСуммы1.Договор = Обороты.Договор
			И НакопительныеСуммы1.ДокументВзаиморасчетов = Обороты.ДокументВзаиморасчетов
			И НакопительныеСуммы1.Период = Обороты.Период
			И НакопительныеСуммы1.Регистратор = Обороты.Регистратор
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Остатки.Договор КАК Договор,
	Остатки.ДокументВзаиморасчетов КАК ДокументВзаиморасчетов,
	Остатки.СуммаОстатокДт,
	НакопительныеСуммы2.Период КАК Период,
	НакопительныеСуммы2.Регистратор,
	НакопительныеСуммы2.НакопительнаяСумма КАК НакопительнаяСуммаНДС,
	НакопительныеСуммы2.СуммаОборот КАК НДСПоДокументу,
	ВЫБОР
		КОГДА Остатки.СуммаОстатокДт &gt;= НакопительныеСуммы2.НакопительнаяСумма
			ТОГДА НакопительныеСуммы2.СуммаОборот
		ИНАЧЕ Остатки.СуммаОстатокДт - (НакопительныеСуммы2.НакопительнаяСумма - НакопительныеСуммы2.СуммаОборот)
	КОНЕЦ КАК СуммаЗадолженностиПоДокументу,
	ВЫБОР
		КОГДА &amp;ДатаОстатков &lt; ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(&amp;ДатаОстатков, МЕСЯЦ), ДЕНЬ, 15)
			ТОГДА ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(ДОБАВИТЬКДАТЕ(&amp;ДатаОстатков, МЕСЯЦ, -1), МЕСЯЦ), ДЕНЬ, 15)
		ИНАЧЕ НАЧАЛОПЕРИОДА(&amp;ДатаОстатков, МЕСЯЦ)
	КОНЕЦ КАК НужнаяДата
ПОМЕСТИТЬ Врем
ИЗ
	Остатки КАК Остатки
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ НакопительныеСуммы2 КАК НакопительныеСуммы2
