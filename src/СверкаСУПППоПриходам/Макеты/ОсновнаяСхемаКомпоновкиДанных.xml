			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	ДокТЧ.Ссылка КАК Документ,
	ДокТЧ.ДоговорКонтрагента КАК ДоговорКонтрагента,
	ДокТЧ.ДоговорКонтрагента.ТипДоговора КАК ТипДоговора,
	ДокТЧ.ДоговорКонтрагента.ВидПосредника КАК ВидПосредника,
	ДокТЧ.Склад КАК Склад,
	"_" + ДокТЧ.Склад.КодПоЕДРПОУ КАК СкладЕДРПОУ,
	ДокТЧ.Номенклатура КАК Номенклатура,
	ДокТЧ.Номенклатура.НоменклатурнаяГруппа КАК НоменклатурнаяГруппа,
	ДокТЧ.Ссылка.Контрагент КАК Контрагент,
	"_" + ДокТЧ.Ссылка.Контрагент.КодПоЕДРПОУ КАК КонтрагентЕДРПОУ,
	ВЫБОР
		КОГДА ДокТЧ.Ссылка.СуммаВключаетНДС
			ТОГДА ДокТЧ.Сумма
		ИНАЧЕ ДокТЧ.Сумма + ДокТЧ.СуммаНДС
	КОНЕЦ КАК СуммаСНДС,
	ДокТЧ.СуммаНДС КАК СуммаНДС,
	ВЫБОР
		КОГДА ДокТЧ.Ссылка.СуммаВключаетНДС
			ТОГДА ДокТЧ.Сумма - ДокТЧ.СуммаНДС
		ИНАЧЕ ДокТЧ.Сумма
	КОНЕЦ КАК СуммаБезНДС,
	ДокТЧ.Количество КАК Количество
ИЗ
	Документ.ПоступлениеТоваровУслуг.Товары КАК ДокТЧ
ГДЕ
	ДокТЧ.Ссылка.Проведен
	И НЕ ДокТЧ.СчетУчетаБУ В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ПроизводственныеЗапасы))
	И ДокТЧ.Ссылка.Дата &gt;= &amp;НачалоПериода
	И ДокТЧ.Ссылка.Дата &lt;= &amp;КонецПериода

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ДокТЧ.Ссылка,
	ДокТЧ.ДоговорКонтрагента,
	ДокТЧ.ДоговорКонтрагента.ТипДоговора,
	ДокТЧ.ДоговорКонтрагента.ВидПосредника,
	ДокТЧ.Склад,
	"_" + ДокТЧ.Склад.КодПоЕДРПОУ,
	ДокТЧ.Номенклатура,
	ДокТЧ.Номенклатура.НоменклатурнаяГруппа,
	ДокТЧ.Ссылка.Контрагент,
	"_" + ДокТЧ.Ссылка.Контрагент.КодПоЕДРПОУ,
	ВЫБОР
		КОГДА ДокТЧ.Ссылка.СуммаВключаетНДС
			ТОГДА ДокТЧ.Сумма
		ИНАЧЕ ДокТЧ.Сумма + ДокТЧ.СуммаНДС
	КОНЕЦ,
	ДокТЧ.СуммаНДС,
	ВЫБОР
		КОГДА ДокТЧ.Ссылка.СуммаВключаетНДС
			ТОГДА ДокТЧ.Сумма - ДокТЧ.СуммаНДС
		ИНАЧЕ ДокТЧ.Сумма
	КОНЕЦ,
	ДокТЧ.Количество
ИЗ
	Документ.ПоступлениеТоваровУслуг.Услуги КАК ДокТЧ
ГДЕ
	ДокТЧ.Ссылка.Проведен
	И ДокТЧ.Ссылка.Дата &gt;= &amp;НачалоПериода
	И ДокТЧ.Ссылка.Дата &lt;= &amp;КонецПериода</query>
		</dataSet>
		<totalField>
			<dataPath>Количество</dataPath>
			<expression>Сумма(Количество)</expression>
		</totalField>
		<totalField>
