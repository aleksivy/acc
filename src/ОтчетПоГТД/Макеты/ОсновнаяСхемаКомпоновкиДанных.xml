				<dataPath>ДатаНН</dataPath>
				<field>ДатаНН</field>
				<title xmlns:d5p1="http://v8.1c.ru/8.1/data/core" xsi:type="d5p1:LocalStringType">
					<d5p1:item>
						<d5p1:lang>ru</d5p1:lang>
						<d5p1:content>Дата НН</d5p1:content>
					</d5p1:item>
				</title>
			</field>
			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	НН.Ссылка КАК НалоговаяНакладная,
	НН.Ссылка.Дата КАК ДатаНН,
	НН.Ссылка.ДокументОснование КАК РасходнаяНакладная,
	ВЫБОР
		КОГДА НН.Ссылка.ДатаПечатиННЭкспорт = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
			ТОГДА НН.Ссылка.ДокументОснование.ДатаПересеченияТамТеритории
		ИНАЧЕ НН.Ссылка.ДатаПечатиННЭкспорт
	КОНЕЦ КАК ДатаПересеченияТамТеритории,
	ВЫБОР
		КОГДА НН.Ссылка.ДатаГТД = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
			ТОГДА НН.Ссылка.ДокументОснование.ДатаГТД
		ИНАЧЕ НН.Ссылка.ДатаГТД
	КОНЕЦ КАК ДатаГТД,
	НН.Ссылка.НомерГТД,
	НН.Сумма КАК СуммаДол,
	НН.Ссылка.КурсВзаиморасчетов / НН.Ссылка.КратностьВзаиморасчетов КАК Курс,
	ВЫРАЗИТЬ(НН.Сумма * НН.Ссылка.КурсВзаиморасчетов / НН.Ссылка.КратностьВзаиморасчетов КАК ЧИСЛО(15, 2)) КАК СуммаГрн,
	НН.Номенклатура,
	НН.Количество,
	НН.Ссылка.ДоговорКонтрагента КАК ДоговорКонтрагента,
	НН.Ссылка.ДокументОснование.Страна КАК Страна
ИЗ
	Документ.НалоговаяНакладная.Товары КАК НН
ГДЕ
	НН.Ссылка.Проведен
	И НЕ НН.Ссылка.ВалютаДокумента = ЗНАЧЕНИЕ(Справочник.Валюты.Гривна)</query>
		</dataSet>
		<totalField>
			<dataPath>СуммаГрн</dataPath>
			<expression>Сумма(СуммаГрн)</expression>
		</totalField>
		<totalField>
			<dataPath>СуммаДол</dataPath>
			<expression>Сумма(СуммаДол)</expression>
		</totalField>
		<totalField>
			<dataPath>Количество</dataPath>
			<expression>Сумма(Количество)</expression>
		</totalField>
		<settingsVariant>
			<name xmlns="http://v8.1c.ru/8.1/data-composition-system/settings">Основной</name>
			<presentation xmlns="http://v8.1c.ru/8.1/data-composition-system/settings" xsi:type="xs:string">Основной</presentation>
		</settingsVariant>
	</dataCompositionSchema>
