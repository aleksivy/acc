	РегистрБухгалтерии.МСФО.Остатки(&amp;Дата, Счет В ИЕРАРХИИ (&amp;СписокСчетов39), , ) КАК Рег

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ЛОЖЬ,
	Рег.Счет,
	Рег.НоменклатурнаяГруппа,
	Рег.Склад,
	Рег.СкладНазначения,
	ВЫБОР
		КОГДА НЕ ПОДСТРОКА(Рег.Счет.Код, 1, 3) = "039"
			ТОГДА NULL
		ИНАЧЕ ВЫБОР
				КОГДА Рег.СтатьяЗатрат В (&amp;СписокЗатратЭкспедирования)
					ТОГДА Рег.СтатьяЗатрат
				ИНАЧЕ ЗНАЧЕНИЕ(Справочник.СтатьиЗатрат.ПустаяСсылка)
			КОНЕЦ
	КОНЕЦ,
	Рег.СуммаДолОстаток,
	Рег.СуммаГрнОстаток,
	Рег.КоличествоОстаток
ИЗ
	РегистрНакопления.СернаРаспределениеТоваров.Остатки(&amp;Дата, ) КАК Рег</query>
		</dataSet>
