	ВЫБОР
		КОГДА ТИПЗНАЧЕНИЯ(Врем.ОС) = ТИП(Справочник.ОсновныеСредства)
			ТОГДА Врем.ОС.ГруппыОС_МСФО
		ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.ГруппыОС_МСФО.IT)
	КОНЕЦ КАК ГруппаМСФО,
	Врем.НалоговоеНазначение,
	Врем.СчетБаланс,
	Врем.СчетИзнос,
	Врем.БалансЛок,
	Врем.ИзносЛок,
	Врем.БалансНУЛок,
	Врем.ИзносНУЛок,
	Врем.СчетБалансМСФО,
	Врем.СчетИзносМСФО,
	Врем.БалансМСФОДол,
	Врем.БалансМСФОГрн,
	Врем.ИзносМСФОДол,
	Врем.ИзносМСФОГрн,
	ЕСТЬNULL(СрокиИсп.СрокПолезногоИспользования, 0) КАК СрокПолезногоИспользования,
	ЕСТЬNULL(СрокиИспБух.СрокПолезногоИспользования, 0) КАК СрокПолезногоИспользованияБух,
	ВЫБОР
		КОГДА ОС_100.ОсновноеСредство ЕСТЬ NULL 
			ТОГДА ЕСТЬNULL(СрокиИспНал.СрокПолезногоИспользования, 0)
		ИНАЧЕ 0
	КОНЕЦ КАК СрокПолезногоИспользованияНал,
	ЕСТЬNULL(ДатыВвода.ДатаВвода, ДАТАВРЕМЯ(1, 1, 1)) КАК ДатаВвода
