			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	Рег.Счет КАК Счет,
	Рег.Субконто1 КАК Субконто1,
	Рег.Субконто2 КАК Субконто2,
	Рег.Субконто3 КАК Субконто3,
	Рег.Организация,
	Рег.Валюта,
	Рег.БИТ_МСФО_ПланСчетов,
	Рег.НалоговоеНазначение,
	Рег.Intercompany,
	Рег.СуммаОстаток,
	Рег.ВалютнаяСуммаОстаток,
	Рег.КоличествоОстаток
ИЗ
	РегистрБухгалтерии.МСФО.Остатки(
			&amp;Дата,
			НЕ Счет.Забалансовый
				И Счет.Вид = ЗНАЧЕНИЕ(ВидСчета.Активный)
				И НЕ ПОДСТРОКА(Счет.Код, 1, 1) = "7"
				И НЕ ПОДСТРОКА(Счет.Код, 1, 1) = "9",
			,
			) КАК Рег
ГДЕ
	(Рег.СуммаОстатокДт &lt; 0
			ИЛИ Рег.ВалютнаяСуммаОстатокДт &lt; 0
			ИЛИ Рег.Счет.Количественный
				И Рег.КоличествоОстатокДт &lt; 0)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Рег.Счет,
	Рег.Субконто1,
	Рег.Субконто2,
	Рег.Субконто3,
	Рег.Организация,
	Рег.Валюта,
	Рег.БИТ_МСФО_ПланСчетов,
	Рег.НалоговоеНазначение,
	Рег.Intercompany,
	Рег.СуммаОстаток,
	Рег.ВалютнаяСуммаОстаток,
	Рег.КоличествоОстаток
ИЗ
	РегистрБухгалтерии.МСФО.Остатки(
			&amp;Дата,
			Счет.Вид = ЗНАЧЕНИЕ(ВидСчета.Пассивный)
				И НЕ ПОДСТРОКА(Счет.Код, 1, 1) = "7"
				И НЕ ПОДСТРОКА(Счет.Код, 1, 1) = "9",
			,
			) КАК Рег
ГДЕ
	(Рег.СуммаОстатокКт &lt; 0
			ИЛИ Рег.ВалютнаяСуммаОстатокКт &lt; 0
			ИЛИ Рег.Счет.Количественный
				И Рег.КоличествоОстатокКт &lt; 0)</query>
		</dataSet>
		<totalField>
			<dataPath>ВалютнаяСуммаОстаток</dataPath>
			<expression>Сумма(ВалютнаяСуммаОстаток)</expression>
		</totalField>
		<totalField>
			<dataPath>КоличествоОстаток</dataPath>
			<expression>Сумма(КоличествоОстаток)</expression>
		</totalField>
		<totalField>
			<dataPath>СуммаОстаток</dataPath>
			<expression>Сумма(СуммаОстаток)</expression>
		</totalField>
