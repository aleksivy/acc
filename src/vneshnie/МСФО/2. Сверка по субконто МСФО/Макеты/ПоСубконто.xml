			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	&amp;Дата КАК ДатаОтчета,
	&amp;Счет КАК Счет,
	Лок.Субконто1 КАК Субконто1Лок,
	МСФО.Субконто1 КАК Субконто1МСФО,
	Лок.Субконто2 КАК Субконто2Лок,
	МСФО.Субконто2 КАК Субконто2МСФО,
	Лок.Субконто3 КАК Субконто3Лок,
	МСФО.Субконто3 КАК Субконто3МСФО,
	ЕСТЬNULL(Лок.СуммаОстаток, 0) - ЕСТЬNULL(МСФО.ВалютнаяСуммаОстаток, 0) КАК Разница,
	ЕСТЬNULL(Лок.СуммаОстаток, 0) КАК СуммаЛок,
	ЕСТЬNULL(МСФО.ВалютнаяСуммаОстаток, 0) КАК СуммаМСФО,
	ВЫБОР
		КОГДА НЕ Лок.Счет.Количественный
				ИЛИ Лок.Счет ЕСТЬ NULL 
			ТОГДА 0
		ИНАЧЕ Лок.КоличествоОстаток
	КОНЕЦ КАК КолвоЛок,
	ВЫБОР
		КОГДА НЕ МСФО.Счет.Количественный
				ИЛИ МСФО.Счет ЕСТЬ NULL 
			ТОГДА 0
		ИНАЧЕ МСФО.КоличествоОстаток
	КОНЕЦ КАК КолвоМСФО
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Остатки(&amp;Дата, Счет В ИЕРАРХИИ (&amp;Счет), , Организация = &amp;Организация) КАК Лок
		ПОЛНОЕ СОЕДИНЕНИЕ РегистрБухгалтерии.МСФО.Остатки(&amp;Дата, Счет В ИЕРАРХИИ (&amp;СчетМСФО), , Организация = &amp;Организация) КАК МСФО
		ПО (ЕСТЬNULL(Лок.Субконто1, "") = ЕСТЬNULL(МСФО.Субконто1, ""))
			И (ЕСТЬNULL(Лок.Субконто2, "") = ЕСТЬNULL(МСФО.Субконто2, ""))
			И (ЕСТЬNULL(Лок.Субконто3, "") = ЕСТЬNULL(МСФО.Субконто3, ""))
ГДЕ
	(НЕ ЕСТЬNULL(МСФО.ВалютнаяСуммаОстаток, 0) = ЕСТЬNULL(Лок.СуммаОстаток, 0)
			ИЛИ Лок.Счет.Количественный
				И НЕ ЕСТЬNULL(МСФО.КоличествоОстаток, 0) = ЕСТЬNULL(Лок.КоличествоОстаток, 0))
	И НЕ(ЕСТЬNULL(МСФО.ВалютнаяСуммаОстаток, 0) = 0
				И ЕСТЬNULL(Лок.СуммаОстаток, 0) = 0
				И ЕСТЬNULL(МСФО.КоличествоОстаток, 0) = 0
				И ЕСТЬNULL(Лок.КоличествоОстаток, 0) = 0)</query>
		</dataSet>
