			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	РегБух.Счет КАК Счет,
	РегБух.Субконто1 КАК Контрагент,
	РегБух.Субконто2 КАК Договор,
	РегБух.СуммаОстатокДт КАК Сумма
ПОМЕСТИТЬ ОстаткиБух
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Остатки(&amp;Дата, Счет = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.РасчетыПоВыданнымАвансамВНациональнойВалюте), , ) КАК РегБух

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	РегБух.Счет,
	РегБух.Субконто1,
	РегБух.Субконто2,
	РегБух.СуммаОстатокКт
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Остатки(&amp;Дата, Счет = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.РасчетыСОтечественнымиПоставщиками), , ) КАК РегБух
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.РасчетыПоВыданнымАвансамВНациональнойВалюте) КАК Счет,
	РегНак.ДоговорКонтрагента.Владелец КАК Контрагент,
	РегНак.ДоговорКонтрагента КАК Договор,
	РегНак.СуммаВзаиморасчетовОстаток КАК Сумма
ПОМЕСТИТЬ ОстаткиРег
ИЗ
	РегистрНакопления.ПриобретенияНалоговыйУчет.Остатки(&amp;Дата, Событие = ЗНАЧЕНИЕ(Перечисление.СобытияПриобретенияНалоговыйУчет.ОплатаПоставщику)) КАК РегНак

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.РасчетыСОтечественнымиПоставщиками),
	РегНак.ДоговорКонтрагента.Владелец,
	РегНак.ДоговорКонтрагента,
	РегНак.СуммаВзаиморасчетовОстаток
ИЗ
	РегистрНакопления.ПриобретенияНалоговыйУчет.Остатки(&amp;Дата, Событие = ЗНАЧЕНИЕ(Перечисление.СобытияПриобретенияНалоговыйУчет.ПоступлениеОтПоставщика)) КАК РегНак
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЕСТЬNULL(ОстаткиБух.Счет, ОстаткиРег.Счет) КАК Счет,
	ЕСТЬNULL(ОстаткиБух.Контрагент, ОстаткиРег.Контрагент) КАК Контрагент,
	ЕСТЬNULL(ОстаткиБух.Договор, ОстаткиРег.Договор) КАК Договор,
	ОстаткиБух.Сумма КАК СуммаБух,
	ОстаткиРег.Сумма КАК СуммаРег,
	ВЫБОР
		КОГДА ЕСТЬNULL(ОстаткиБух.Сумма, 0) = ЕСТЬNULL(ОстаткиРег.Сумма, 0)
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК СуммыСовпадают,
	ЕСТЬNULL(ОстаткиБух.Сумма, 0) - ЕСТЬNULL(ОстаткиРег.Сумма, 0) КАК Разница
ИЗ
	ОстаткиБух КАК ОстаткиБух
		ПОЛНОЕ СОЕДИНЕНИЕ ОстаткиРег КАК ОстаткиРег
		ПО ОстаткиБух.Счет = ОстаткиРег.Счет
			И ОстаткиБух.Контрагент = ОстаткиРег.Контрагент
			И ОстаткиБух.Договор = ОстаткиРег.Договор</query>
		</dataSet>
		<parameter>
			<name>Период</name>
			<title xmlns:d4p1="http://v8.1c.ru/8.1/data/core" xsi:type="d4p1:LocalStringType">
				<d4p1:item>
					<d4p1:lang>ru</d4p1:lang>
					<d4p1:content>Период</d4p1:content>
				</d4p1:item>
			</title>
			<valueType>
				<Type xmlns="http://v8.1c.ru/8.1/data/core">xs:dateTime</Type>
				<DateQualifiers xmlns="http://v8.1c.ru/8.1/data/core">
					<DateFractions>DateTime</DateFractions>
				</DateQualifiers>
			</valueType>
			<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
			<useRestriction>false</useRestriction>
		</parameter>
		<parameter>
			<name>Дата</name>
			<title xmlns:d4p1="http://v8.1c.ru/8.1/data/core" xsi:type="d4p1:LocalStringType">
				<d4p1:item>
					<d4p1:lang>ru</d4p1:lang>
					<d4p1:content>Дата</d4p1:content>
				</d4p1:item>
			</title>
			<valueType>
				<Type xmlns="http://v8.1c.ru/8.1/data/core">xs:dateTime</Type>
				<DateQualifiers xmlns="http://v8.1c.ru/8.1/data/core">
					<DateFractions>DateTime</DateFractions>
				</DateQualifiers>
			</valueType>
			<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
			<useRestriction>true</useRestriction>
		</parameter>
