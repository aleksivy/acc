ВЫБРАТЬ
	ДокТЧ.Ссылка,
	ДокТЧ.НалоговоеНазначение,
	ДокТЧ.СтавкаНДС,
	ДокТЧ.Номенклатура,
	ДокТЧ.Склад,
	ДокТЧ.ДоговорКонтрагента
ПОМЕСТИТЬ ПоступлениеТоваровУслугТовары
ИЗ
	Документ.ПоступлениеТоваровУслуг.Товары КАК ДокТЧ

СГРУППИРОВАТЬ ПО
	ДокТЧ.НалоговоеНазначение,
	ДокТЧ.ДоговорКонтрагента,
	ДокТЧ.Ссылка,
	ДокТЧ.СтавкаНДС,
	ДокТЧ.Номенклатура,
	ДокТЧ.Склад
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Рег.Регистратор,
	Рег.СчетДт КАК Счет,
	Рег.СубконтоДт1 КАК Номенклатура,
	Рег.СубконтоДт2 КАК Склад,
	Рег.СубконтоДт3 КАК Партия,
	Рег.СубконтоКт1 КАК Контрагент,
	Рег.СубконтоКт2 КАК Договор,
	Рег.СубконтоКт2.ТипДоговора КАК ТипДоговора,
	Рег.СубконтоКт2.ВидПосредника КАК ВидПосредника,
	Рег.СубконтоКт3 КАК ДокументРасчетов,
	ДокТЧ.НалоговоеНазначение,
	ДокТЧ.СтавкаНДС,
	Рег.КоличествоДт КАК Количество,
	Рег.Сумма
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ДвиженияССубконто(
			&amp;НачалоПериода,
			&amp;КонецПериода,
			СчетКт = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.РасчетыСОтечественнымиПоставщиками)
				И (СчетДт В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ПроизводственныеЗапасы))
					ИЛИ СчетДт В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ТоварыНаСкладе))),
			,
			) КАК Рег
		ЛЕВОЕ СОЕДИНЕНИЕ ПоступлениеТоваровУслугТовары КАК ДокТЧ
		ПО Рег.Регистратор = ДокТЧ.Ссылка
			И Рег.СубконтоДт1 = ДокТЧ.Номенклатура
			И Рег.СубконтоДт2 = ДокТЧ.Склад
			И Рег.СубконтоКт2 = ДокТЧ.ДоговорКонтрагента

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Рег.Регистратор,
	Рег.СчетКт,
	Рег.СубконтоКт1,
	Рег.СубконтоКт2,
	Рег.СубконтоКт3,
	Рег.СубконтоДт1,
	Рег.СубконтоДт2,
	Рег.СубконтоДт2.ТипДоговора,
	Рег.СубконтоДт2.ВидПосредника,
	Рег.СубконтоДт3,
	ДокТЧ.НалоговоеНазначение,
	ДокТЧ.СтавкаНДС,
	-Рег.КоличествоКт,
	-Рег.Сумма
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ДвиженияССубконто(
			&amp;НачалоПериода,
			&amp;КонецПериода,
			СчетДт = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.РасчетыСОтечественнымиПоставщиками)
				И (СчетКт В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ПроизводственныеЗапасы))
					ИЛИ СчетКт В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ТоварыНаСкладе))),
			,
			) КАК Рег
		ЛЕВОЕ СОЕДИНЕНИЕ ВозвратТоваровПоставщикуТовары КАК ДокТЧ
		ПО Рег.Регистратор = ДокТЧ.Ссылка
			И Рег.СубконтоКт1 = ДокТЧ.Номенклатура
			И Рег.СубконтоКт2 = ДокТЧ.Склад
			И Рег.СубконтоДт2 = ДокТЧ.ДоговорКонтрагента</query>
		</dataSet>
		<totalField>
			<dataPath>Сумма</dataPath>
			<expression>Сумма(Сумма)</expression>
		</totalField>
		<totalField>
