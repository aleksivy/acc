			<field xsi:type="DataSetFieldField">
				<dataPath>Количество</dataPath>
				<field>Количество</field>
				<title xmlns:d5p1="http://v8.1c.ru/8.1/data/core" xsi:type="d5p1:LocalStringType">
					<d5p1:item>
						<d5p1:lang>ru</d5p1:lang>
						<d5p1:content>Кол-во</d5p1:content>
					</d5p1:item>
				</title>
			</field>
			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	Рег.Регистратор,
	Рег.Субконто1 КАК Контрагент,
	Рег.Субконто2.Номенклатура КАК ТМЦ,
	Рег.КорСчет,
	ВЫБОР
		КОГДА ТИПЗНАЧЕНИЯ(Рег.КорСубконто1) = ТИП(Справочник.СтатьиЗатрат)
			ТОГДА Рег.КорСубконто1
		КОГДА ТИПЗНАЧЕНИЯ(Рег.КорСубконто2) = ТИП(Справочник.СтатьиЗатрат)
			ТОГДА Рег.КорСубконто2
		КОГДА ТИПЗНАЧЕНИЯ(Рег.КорСубконто3) = ТИП(Справочник.СтатьиЗатрат)
			ТОГДА Рег.КорСубконто3
		ИНАЧЕ ЗНАЧЕНИЕ(Справочник.СтатьиЗатрат.ПустаяСсылка)
	КОНЕЦ КАК СтатьяЗатрат,
	-Рег.СуммаОборот КАК Сумма,
	-Рег.ВалютнаяСуммаОборот КАК СуммаГрн,
	-Рег.КоличествоОборот КАК Количество
ИЗ
	РегистрБухгалтерии.МСФО.Обороты(
			&amp;НачалоПериода,
			&amp;КонецПериода,
			Регистратор,
			Счет.Код = "631"
				ИЛИ Счет.Код = "6851",
			,
			,
			НЕ КорСчет.Код = "3711"
				И НЕ КорСчет.Код = "311",
			) КАК Рег</query>
		</dataSet>
		<totalField>
			<dataPath>Сумма</dataPath>
			<expression>Сумма(Сумма)</expression>
		</totalField>
		<totalField>
			<dataPath>СуммаГрн</dataPath>
			<expression>Сумма(СуммаГрн)</expression>
		</totalField>
		<totalField>
			<dataPath>Количество</dataPath>
			<expression>Сумма(Количество)</expression>
		</totalField>
