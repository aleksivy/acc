				<title xmlns:d5p1="http://v8.1c.ru/8.1/data/core" xsi:type="d5p1:LocalStringType">
					<d5p1:item>
						<d5p1:lang>ru</d5p1:lang>
						<d5p1:content>Суммы равны</d5p1:content>
					</d5p1:item>
				</title>
				<appearance/>
				<inputParameters/>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>НалоговоеНазначение</dataPath>
				<field>НалоговоеНазначение</field>
				<appearance/>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>Валюта</dataPath>
				<field>Валюта</field>
				<appearance/>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>Период</dataPath>
				<field>Период</field>
				<title xmlns:d5p1="http://v8.1c.ru/8.1/data/core" xsi:type="d5p1:LocalStringType">
					<d5p1:item>
						<d5p1:lang>ru</d5p1:lang>
						<d5p1:content>Период</d5p1:content>
					</d5p1:item>
				</title>
				<role>
					<periodNumber xmlns="http://v8.1c.ru/8.1/data-composition-system/common">2</periodNumber>
					<periodType xmlns="http://v8.1c.ru/8.1/data-composition-system/common">Main</periodType>
				</role>
				<appearance>
					<item xmlns="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xsi:type="dcsset:SettingsParameterValue">
						<parameter>Формат</parameter>
						<value xmlns:d7p1="http://v8.1c.ru/8.1/data/core" xsi:type="d7p1:LocalStringType">
							<d7p1:item>
								<d7p1:lang>ru</d7p1:lang>
								<d7p1:content>ДФ=dd.MM.yyyy</d7p1:content>
							</d7p1:item>
						</value>
					</item>
				</appearance>
			</field>
			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	Рег.Субконто1,
	Рег.Субконто2,
	Рег.СуммаОстаток
ПОМЕСТИТЬ ПоДоговоруВЦелом
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Остатки(КОНЕЦПЕРИОДА(&amp;ДатаОтчета, ДЕНЬ), Счет = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.НалоговыйКредитНеподтвержденный), , НЕ Субконто2.ВедениеВзаиморасчетов = ЗНАЧЕНИЕ(Перечисление.ВедениеВзаиморасчетовПоДоговорам.ПоРасчетнымДокументам)) КАК Рег
ГДЕ
	Рег.СуммаОстаток &gt; 10
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Рег.Период,
	Рег.Регистратор,
	Рег.Субконто1,
	Рег.Субконто2,
	Рег.СуммаНачальныйОстаток,
	Рег.СуммаОборот,
	Рег.СуммаКонечныйОстаток
ПОМЕСТИТЬ ОстаткиПоДокам
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ОстаткиИОбороты(
			ДАТАВРЕМЯ(2017, 6, 1),
			КОНЕЦПЕРИОДА(&amp;ДатаОтчета, ДЕНЬ),
			Регистратор,
			,
			Счет = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.НалоговыйКредитНеподтвержденный),
			,
			Субконто2 В
				(ВЫБРАТЬ
					ПоДоговоруВЦелом.Субконто2
				ИЗ
					ПоДоговоруВЦелом КАК ПоДоговоруВЦелом)) КАК Рег
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ОстаткиПоДокам.Субконто1,
	ОстаткиПоДокам.Субконто2,
	МАКСИМУМ(ОстаткиПоДокам.Период) КАК ПериодМакс
ПОМЕСТИТЬ ПериодыМакс
ИЗ
	ОстаткиПоДокам КАК ОстаткиПоДокам
ГДЕ
	ОстаткиПоДокам.СуммаКонечныйОстаток &lt;= 0

СГРУППИРОВАТЬ ПО
	ОстаткиПоДокам.Субконто1,
	ОстаткиПоДокам.Субконто2
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ОстаткиПоДокам.Период,
	ОстаткиПоДокам.Регистратор,
	ОстаткиПоДокам.Субконто1,
	ОстаткиПоДокам.Субконто2,
	ОстаткиПоДокам.СуммаОборот,
	ОстаткиПоДокам.СуммаКонечныйОстаток
ПОМЕСТИТЬ НужныеДоки1
ИЗ
	ОстаткиПоДокам КАК ОстаткиПоДокам
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ПериодыМакс КАК ПериодыМакс
		ПО ОстаткиПоДокам.Субконто1 = ПериодыМакс.Субконто1
			И ОстаткиПоДокам.Субконто2 = ПериодыМакс.Субконто2
			И ОстаткиПоДокам.Период &gt; ПериодыМакс.ПериодМакс
ГДЕ
	ОстаткиПоДокам.Регистратор &lt;&gt; НЕОПРЕДЕЛЕНО
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ОстаткиПоДокам.Период,
	ОстаткиПоДокам.Регистратор,
	ОстаткиПоДокам.Субконто1,
	ОстаткиПоДокам.Субконто2,
	ОстаткиПоДокам.СуммаОборот,
	ОстаткиПоДокам.СуммаКонечныйОстаток
ПОМЕСТИТЬ НужныеДоки2
ИЗ
	ОстаткиПоДокам КАК ОстаткиПоДокам
		ЛЕВОЕ СОЕДИНЕНИЕ ПериодыМакс КАК ПериодыМакс
		ПО ОстаткиПоДокам.Субконто1 = ПериодыМакс.Субконто1
			И ОстаткиПоДокам.Субконто2 = ПериодыМакс.Субконто2
			И ОстаткиПоДокам.Период &gt; ПериодыМакс.ПериодМакс
		ЛЕВОЕ СОЕДИНЕНИЕ НужныеДоки1 КАК НужныеДоки1
		ПО ОстаткиПоДокам.Субконто1 = НужныеДоки1.Субконто1
			И ОстаткиПоДокам.Субконто2 = НужныеДоки1.Субконто2
ГДЕ
	ОстаткиПоДокам.Регистратор &lt;&gt; НЕОПРЕДЕЛЕНО
	И ПериодыМакс.ПериодМакс ЕСТЬ NULL
	И НужныеДоки1.Субконто2 ЕСТЬ NULL
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	НужныеДоки.Период,
	НужныеДоки.Регистратор,
	НужныеДоки.Субконто1,
	НужныеДоки.Субконто2,
	НужныеДоки.СуммаОборот,
	НужныеДоки.СуммаКонечныйОстаток
ПОМЕСТИТЬ НужныеДоки
ИЗ
	НужныеДоки1 КАК НужныеДоки

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	НужныеДоки.Период,
	НужныеДоки.Регистратор,
	НужныеДоки.Субконто1,
	НужныеДоки.Субконто2,
	НужныеДоки.СуммаОборот,
	НужныеДоки.СуммаКонечныйОстаток
ИЗ
	НужныеДоки2 КАК НужныеДоки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	НужныеДоки.Субконто1,
	НужныеДоки.Субконто2,
	СУММА(НужныеДоки.СуммаОборот) КАК СуммаОборот
ПОМЕСТИТЬ НужныеДокиСвернуто
ИЗ
	НужныеДоки КАК НужныеДоки

СГРУППИРОВАТЬ ПО
	НужныеДоки.Субконто1,
	НужныеДоки.Субконто2
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЛОЖЬ КАК ПоРасчетным,
	ПоДоговоруВЦелом.Субконто1,
	ПоДоговоруВЦелом.Субконто2,
	ПоДоговоруВЦелом.СуммаОстаток,
	ВЫБОР
		КОГДА ПоДоговоруВЦелом.СуммаОстаток = ЕСТЬNULL(НужныеДокиСвернуто.СуммаОборот, 0)
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК СуммыРавны,
	ЕСТЬNULL(НужныеДокиСвернуто.СуммаОборот, 0) КАК СуммаПоДокамСвернуто,
	ЕСТЬNULL(НужныеДоки.Период, ДАТАВРЕМЯ(1, 1, 1)) КАК Период,
	ЕСТЬNULL(НужныеДоки.Регистратор, НЕОПРЕДЕЛЕНО) КАК Регистратор,
	ЕСТЬNULL(НужныеДоки.СуммаОборот, 0) КАК СуммаПоДоку
ПОМЕСТИТЬ Врем1
ИЗ
	ПоДоговоруВЦелом КАК ПоДоговоруВЦелом
		ЛЕВОЕ СОЕДИНЕНИЕ НужныеДокиСвернуто КАК НужныеДокиСвернуто
		ПО ПоДоговоруВЦелом.Субконто1 = НужныеДокиСвернуто.Субконто1
			И ПоДоговоруВЦелом.Субконто2 = НужныеДокиСвернуто.Субконто2
		ЛЕВОЕ СОЕДИНЕНИЕ НужныеДоки КАК НужныеДоки
		ПО ПоДоговоруВЦелом.Субконто1 = НужныеДоки.Субконто1
			И ПоДоговоруВЦелом.Субконто2 = НужныеДоки.Субконто2
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ИСТИНА КАК ПоРасчетным,
	Рег.Субконто1,
	Рег.Субконто2,
	Рег.СуммаОстаток,
	ИСТИНА КАК СуммыРавны,
