			<field xsi:type="DataSetFieldField">
				<dataPath>ЦенаГрн</dataPath>
				<field>ЦенаГрн</field>
				<title xmlns:d5p1="http://v8.1c.ru/8.1/data/core" xsi:type="d5p1:LocalStringType">
					<d5p1:item>
						<d5p1:lang>ru</d5p1:lang>
						<d5p1:content>Цена грн</d5p1:content>
					</d5p1:item>
				</title>
				<appearance>
					<item xmlns="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xsi:type="dcsset:SettingsParameterValue">
						<parameter>Формат</parameter>
						<value xmlns:d7p1="http://v8.1c.ru/8.1/data/core" xsi:type="d7p1:LocalStringType">
							<d7p1:item>
								<d7p1:lang>ru</d7p1:lang>
								<d7p1:content>ЧДЦ=2</d7p1:content>
							</d7p1:item>
						</value>
					</item>
				</appearance>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>ЦенаДол</dataPath>
				<field>ЦенаДол</field>
				<title xmlns:d5p1="http://v8.1c.ru/8.1/data/core" xsi:type="d5p1:LocalStringType">
					<d5p1:item>
						<d5p1:lang>ru</d5p1:lang>
						<d5p1:content>Цена дол</d5p1:content>
					</d5p1:item>
				</title>
				<appearance>
					<item xmlns="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xsi:type="dcsset:SettingsParameterValue">
						<parameter>Формат</parameter>
						<value xmlns:d7p1="http://v8.1c.ru/8.1/data/core" xsi:type="d7p1:LocalStringType">
							<d7p1:item>
								<d7p1:lang>ru</d7p1:lang>
								<d7p1:content>ЧДЦ=2</d7p1:content>
							</d7p1:item>
						</value>
					</item>
				</appearance>
			</field>
			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	СводнаяПродажа.РасходнаяНакладная,
	СводнаяПродажа.ДоговорРеализации,
	СводнаяПродажа.Номенклатура,
	СводнаяПродажа.СчетДоходов,
	СУММА(СводнаяПродажа.Количество) КАК Количество,
	СУММА(СводнаяПродажа.Сумма) КАК Сумма
ПОМЕСТИТЬ РеализацияДокТЧ1
ИЗ
	(ВЫБРАТЬ
		ДокТЧ.Ссылка КАК РасходнаяНакладная,
		ДокТЧ.ДоговорКонтрагента КАК ДоговорРеализации,
		ДокТЧ.Номенклатура КАК Номенклатура,
		ДокТЧ.СхемаРеализации.СчетДоходов.Код КАК СчетДоходов,
		СУММА(ДокТЧ.Количество) КАК Количество,
		ВЫБОР
			КОГДА ДокТЧ.Ссылка.СуммаВключаетНДС
				ТОГДА СУММА(ДокТЧ.Сумма) - СУММА(ДокТЧ.СуммаНДС)
			ИНАЧЕ СУММА(ДокТЧ.Сумма)
		КОНЕЦ КАК Сумма
	ИЗ
		Документ.РеализацияТоваровУслуг.Товары КАК ДокТЧ
	ГДЕ
		НЕ ДокТЧ.Ссылка.КорректировкаРеализации
		И ДокТЧ.Ссылка.Дата &gt;= НАЧАЛОПЕРИОДА(&amp;НачПериода, ДЕНЬ)
		И ДокТЧ.Ссылка.Дата &lt;= КОНЕЦПЕРИОДА(&amp;КонПериода, ДЕНЬ)
		И (ДокТЧ.ДоговорКонтрагента = &amp;Договор
				ИЛИ &amp;Договор = ЗНАЧЕНИЕ(Справочник.ДоговорыКонтрагентов.ПустаяСсылка))
		И (ДокТЧ.ДоговорКонтрагента.Владелец = &amp;Контрагент
				ИЛИ &amp;Контрагент = ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка))
	
	СГРУППИРОВАТЬ ПО
		ДокТЧ.Ссылка,
		ДокТЧ.ДоговорКонтрагента,
		ДокТЧ.Номенклатура,
		ДокТЧ.СхемаРеализации.СчетДоходов.Код,
		ДокТЧ.Ссылка.СуммаВключаетНДС
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ
		ДокТЧ.Ссылка.ДокументОснование,
		ДокТЧ.ДоговорКонтрагента,
		ДокТЧ.Номенклатура,
		ДокТЧ.СхемаРеализации.СчетДоходов.Код,
		0,
		ВЫБОР
			КОГДА ДокТЧ.Ссылка.СуммаВключаетНДС
				ТОГДА СУММА(ДокТЧ.Сумма) - СУММА(ДокТЧ.СуммаНДС)
			ИНАЧЕ СУММА(ДокТЧ.Сумма)
		КОНЕЦ
	ИЗ
		Документ.РеализацияТоваровУслуг.Товары КАК ДокТЧ
	ГДЕ
		ДокТЧ.Ссылка.КорректировкаРеализации
		И ДокТЧ.Ссылка.Дата &gt;= НАЧАЛОПЕРИОДА(&amp;НачПериода, ДЕНЬ)
		И ДокТЧ.Ссылка.Дата &lt;= КОНЕЦПЕРИОДА(&amp;КонПериода, ДЕНЬ)
		И (ДокТЧ.ДоговорКонтрагента = &amp;Договор
				ИЛИ &amp;Договор = ЗНАЧЕНИЕ(Справочник.ДоговорыКонтрагентов.ПустаяСсылка))
		И (ДокТЧ.ДоговорКонтрагента.Владелец = &amp;Контрагент
				ИЛИ &amp;Контрагент = ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка))
	
	СГРУППИРОВАТЬ ПО
		ДокТЧ.Ссылка,
		ДокТЧ.ДоговорКонтрагента,
		ДокТЧ.Номенклатура,
		ДокТЧ.СхемаРеализации.СчетДоходов.Код,
		ДокТЧ.Ссылка.ДокументОснование,
		ДокТЧ.Ссылка.СуммаВключаетНДС) КАК СводнаяПродажа

СГРУППИРОВАТЬ ПО
	СводнаяПродажа.РасходнаяНакладная,
	СводнаяПродажа.ДоговорРеализации,
	СводнаяПродажа.Номенклатура,
	СводнаяПродажа.СчетДоходов
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	РеализацияДокТЧ.РасходнаяНакладная,
	РеализацияДокТЧ.ДоговорРеализации,
	РеализацияДокТЧ.Номенклатура,
	РеализацияДокТЧ.СчетДоходов,
	ВЫБОР
		КОГДА РеализацияДокТЧ.РасходнаяНакладная.ВалютаДокумента = ЗНАЧЕНИЕ(Справочник.Валюты.Доллар)
			ТОГДА ВЫБОР
					КОГДА РеализацияДокТЧ.Количество = 0
						ТОГДА 0
					ИНАЧЕ РеализацияДокТЧ.Сумма / РеализацияДокТЧ.Количество
				КОНЕЦ
		ИНАЧЕ 0
	КОНЕЦ КАК ЦенаДол,
	ВЫБОР
		КОГДА РеализацияДокТЧ.РасходнаяНакладная.ВалютаДокумента = ЗНАЧЕНИЕ(Справочник.Валюты.Доллар)
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА РеализацияДокТЧ.Количество = 0
					ТОГДА 0
				ИНАЧЕ РеализацияДокТЧ.Сумма / РеализацияДокТЧ.Количество
			КОНЕЦ
	КОНЕЦ КАК ЦенаГрн
ПОМЕСТИТЬ РеализацияДокТЧ
ИЗ
	РеализацияДокТЧ1 КАК РеализацияДокТЧ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВЫБОР
		КОГДА Рег.Регистратор.КорректировкаРеализации
			ТОГДА Рег.Регистратор.ДокументОснование
		ИНАЧЕ Рег.Регистратор
	КОНЕЦ КАК РасходнаяНакладная,
	Рег.СубконтоКт2 КАК ДоговорРеализации,
	Рег.СчетКт.Код КАК СчетДоходов,
	ВЫБОР
		КОГДА СУММА(Рег.Сумма) = 0
			ТОГДА 0
		ИНАЧЕ СУММА(Рег.ВалютнаяСуммаДт) / СУММА(Рег.Сумма)
	КОНЕЦ КАК Курс,
	Рег.IntercompanyКт
ПОМЕСТИТЬ КурсРеализации
ИЗ
	РегистрБухгалтерии.МСФО.ДвиженияССубконто(
			НАЧАЛОПЕРИОДА(&amp;НачПериода, ДЕНЬ),
			КОНЕЦПЕРИОДА(&amp;КонПериода, ДЕНЬ),
			ТИПЗНАЧЕНИЯ(Регистратор) = ТИП(Документ.РеализацияТоваровУслуг)
				И ПОДСТРОКА(СчетКт.Код, 1, 2) = "70"
				И ПОДСТРОКА(СчетДт.Код, 1, 2) = "36"
				И (СубконтоКт2 = &amp;Договор
					ИЛИ &amp;Договор = ЗНАЧЕНИЕ(Справочник.ДоговорыКонтрагентов.ПустаяСсылка))
				И (СубконтоКт2.Владелец = &amp;Контрагент
					ИЛИ &amp;Контрагент = ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)),
			,
			) КАК Рег

СГРУППИРОВАТЬ ПО
	ВЫБОР
		КОГДА Рег.Регистратор.КорректировкаРеализации
			ТОГДА Рег.Регистратор.ДокументОснование
		ИНАЧЕ Рег.Регистратор
	КОНЕЦ,
	Рег.СубконтоКт2,
	Рег.СчетКт.Код,
	Рег.IntercompanyКт
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	РеализацияДокТЧ.РасходнаяНакладная,
	РеализацияДокТЧ.ДоговорРеализации,
	РеализацияДокТЧ.Номенклатура,
	ВЫБОР
		КОГДА РеализацияДокТЧ.РасходнаяНакладная.ВалютаДокумента = ЗНАЧЕНИЕ(Справочник.Валюты.Доллар)
			ТОГДА РеализацияДокТЧ.ЦенаДол
		КОГДА КурсРеализации.Курс = 0
			ТОГДА 0
		ИНАЧЕ РеализацияДокТЧ.ЦенаГрн / КурсРеализации.Курс
	КОНЕЦ КАК ЦенаДол,
	ВЫБОР
		КОГДА РеализацияДокТЧ.РасходнаяНакладная.ВалютаДокумента = ЗНАЧЕНИЕ(Справочник.Валюты.Доллар)
			ТОГДА РеализацияДокТЧ.ЦенаДол * КурсРеализации.Курс
		ИНАЧЕ РеализацияДокТЧ.ЦенаГрн
	КОНЕЦ КАК ЦенаГрн,
	КурсРеализации.IntercompanyКт КАК Intercompany
ПОМЕСТИТЬ ЦеныРеализации
ИЗ
	РеализацияДокТЧ КАК РеализацияДокТЧ
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ КурсРеализации КАК КурсРеализации
		ПО РеализацияДокТЧ.РасходнаяНакладная = КурсРеализации.РасходнаяНакладная
			И РеализацияДокТЧ.ДоговорРеализации = КурсРеализации.ДоговорРеализации
			И РеализацияДокТЧ.СчетДоходов = КурсРеализации.СчетДоходов
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Рег.Регистратор КАК РасходнаяНакладная,
	Рег.СубконтоДт2 КАК ДоговорРеализации,
	Рег.СубконтоКт1 КАК Номенклатура,
	Рег.НалоговоеНазначениеКт КАК НалоговоеНазначение,
	Рег.СубконтоКт2 КАК Склад,
	Рег.СубконтоКт3 КАК Партия,
	ВЫБОР
		КОГДА Рег.Регистратор = Рег.СубконтоКт3
			ТОГДА ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)
		ИНАЧЕ Рег.СубконтоКт3.Контрагент
	КОНЕЦ КАК КонтрагентПоставщик,
	ВЫБОР
		КОГДА Рег.Регистратор = Рег.СубконтоКт3
			ТОГДА ЗНАЧЕНИЕ(Справочник.ДоговорыКонтрагентов.ПустаяСсылка)
		ИНАЧЕ ВЫБОР
				КОГДА Рег.СубконтоКт3.ДоговорКонтрагента = ЗНАЧЕНИЕ(Справочник.ДоговорыКонтрагентов.ПустаяСсылка)
					ТОГДА НЕОПРЕДЕЛЕНО
				ИНАЧЕ Рег.СубконтоКт3.ДоговорКонтрагента
			КОНЕЦ
	КОНЕЦ КАК ДоговорПокупки,
	Рег.Сумма КАК СебестоимостьДол,
	Рег.ВалютнаяСуммаКт КАК СебестоимостьГрн,
	Рег.КоличествоКт КАК Количество,
	ЕСТЬNULL(ЦеныРеализации.ЦенаДол, 0) КАК ЦенаДол,
	ЕСТЬNULL(ЦеныРеализации.ЦенаГрн, 0) КАК ЦенаГрн,
	ЦеныРеализации.Intercompany
ПОМЕСТИТЬ ПроданныеПартии
ИЗ
	РегистрБухгалтерии.МСФО.ДвиженияССубконто(
			НАЧАЛОПЕРИОДА(&amp;НачПериода, ДЕНЬ),
			КОНЕЦПЕРИОДА(&amp;КонПериода, ДЕНЬ),
			ТИПЗНАЧЕНИЯ(Регистратор) = ТИП(Документ.РеализацияТоваровУслуг)
				И ПОДСТРОКА(СчетДт.Код, 1, 2) = "90"
				И (ПОДСТРОКА(СчетКт.Код, 1, 2) = "28"
					ИЛИ ПОДСТРОКА(СчетКт.Код, 1, 2) = "26")
				И (СубконтоДт2 = &amp;Договор
					ИЛИ &amp;Договор = ЗНАЧЕНИЕ(Справочник.ДоговорыКонтрагентов.ПустаяСсылка))
				И (СубконтоДт2.Владелец = &amp;Контрагент
					ИЛИ &amp;Контрагент = ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)),
			,
			) КАК Рег
		ЛЕВОЕ СОЕДИНЕНИЕ ЦеныРеализации КАК ЦеныРеализации
		ПО Рег.Регистратор = ЦеныРеализации.РасходнаяНакладная
			И Рег.СубконтоДт2 = ЦеныРеализации.ДоговорРеализации
			И Рег.СубконтоКт1 = ЦеныРеализации.Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДокТЧ.Ссылка КАК ПриходнаяНакладная,
	ДокТЧ.Номенклатура,
	ДокТЧ.ДоговорКонтрагента КАК ДоговорКонтрагента,
	СУММА(ДокТЧ.Количество) КАК Количество
ПОМЕСТИТЬ Приходы
ИЗ
	Документ.ПоступлениеТоваровУслуг.Товары КАК ДокТЧ
ГДЕ
	ДокТЧ.Ссылка.ДоговорКонтрагента = ЗНАЧЕНИЕ(Справочник.ДоговорыКонтрагентов.ПустаяСсылка)
	И ДокТЧ.Ссылка В
			(ВЫБРАТЬ
				ПроданныеПартии.Партия
			ИЗ
				ПроданныеПартии КАК ПроданныеПартии)

СГРУППИРОВАТЬ ПО
	ДокТЧ.Ссылка,
	ДокТЧ.Номенклатура,
	ДокТЧ.ДоговорКонтрагента
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Приходы.ПриходнаяНакладная,
	Приходы.Номенклатура,
	МАКСИМУМ(Приходы.Количество) КАК Количество
ПОМЕСТИТЬ ПриходыМаксимальныеКолва
ИЗ
	Приходы КАК Приходы

СГРУППИРОВАТЬ ПО
	Приходы.ПриходнаяНакладная,
	Приходы.Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Приходы.ПриходнаяНакладная,
	Приходы.Номенклатура,
	Приходы.ДоговорКонтрагента
ПОМЕСТИТЬ ДоговораПоПриходам
ИЗ
	Приходы КАК Приходы
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ПриходыМаксимальныеКолва КАК ПриходыМаксимальныеКолва
		ПО Приходы.ПриходнаяНакладная = ПриходыМаксимальныеКолва.ПриходнаяНакладная
			И Приходы.Количество = ПриходыМаксимальныеКолва.Количество
			И Приходы.Номенклатура = ПриходыМаксимальныеКолва.Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Рег.РасходнаяНакладная,
	Рег.РасходнаяНакладная.Дата КАК ДатаРеализации,
	Рег.РасходнаяНакладная.УсловияПоставки КАК УсловияПоставки,
	Рег.РасходнаяНакладная.Теплоход КАК Теплоход,
	Рег.РасходнаяНакладная.ВалютаДокумента КАК ВалютаРеализации,
