				</role>
			</field>
			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	Рег.Регистратор,
	Рег.СубконтоКт2 КАК ДоговорКонтрагента,
	Рег.СубконтоКт2.Номенклатура КАК Номенклатура,
	СУММА(Рег.Сумма) КАК Сумма,
	СУММА(Рег.КоличествоДт) КАК Количество
ПОМЕСТИТЬ Цены1
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ДвиженияССубконто(
			,
			,
			ТИПЗНАЧЕНИЯ(Регистратор) = ТИП(Документ.РеализацияТоваровУслуг)
				И СчетКт В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ДоходыОтРеализации))
				И СчетДт В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.РасчетыСПокупателямиИЗаказчиками)),
			,
			) КАК Рег

СГРУППИРОВАТЬ ПО
	Рег.Регистратор,
	Рег.СубконтоКт2,
	Рег.СубконтоКт2.Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Цены.Регистратор,
	Цены.ДоговорКонтрагента,
	Цены.Номенклатура,
	Цены.Сумма КАК Сумма,
	Цены.Количество КАК Количество,
	ВЫБОР
		КОГДА Цены.Количество &lt; 1
			ТОГДА 0
		ИНАЧЕ ВЫРАЗИТЬ(Цены.Сумма / Цены.Количество КАК ЧИСЛО(12, 5))
	КОНЕЦ КАК Цена
ПОМЕСТИТЬ Цены
ИЗ
	Цены1 КАК Цены
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Рег.Регистратор,
	Рег.ДоговорКонтрагента КАК ДоговорКонтрагента,
	ВЫБОР
		КОГДА СУММА(Рег.Количество) &lt; 1
			ТОГДА 0
		ИНАЧЕ ВЫРАЗИТЬ(СУММА(Рег.Сумма) / СУММА(Рег.Количество) КАК ЧИСЛО(12, 5))
	КОНЕЦ КАК Цена
ПОМЕСТИТЬ ЦеныПоДоговору
ИЗ
	Цены КАК Рег

СГРУППИРОВАТЬ ПО
	Рег.Регистратор,
	Рег.ДоговорКонтрагента
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Рег.Регистратор КАК Документ,
	Рег.Регистратор.Контрагент КАК Покупатель,
	Рег.СубконтоКт3.ДоговорКонтрагента.Номенклатура.ОсвобожденоОтНДС КАК ОсвобожденоОтНДС,
	Рег.СубконтоКт3.ДоговорКонтрагента.ВидПосредника КАК ВидПосредника,
	Рег.СубконтоКт3.ДоговорКонтрагента.Номенклатура КАК Номенклатура,
	Рег.СубконтоКт3.ДоговорКонтрагента.Владелец КАК Поставщик,
	Рег.СубконтоКт3.ДоговорКонтрагента КАК ДоговорКонтрагента,
	Рег.СубконтоКт2 КАК Склад,
	Рег.СубконтоКт3 КАК Партия,
	Рег.СубконтоКт3.Дата КАК ДатаОприходования,
	Рег.КоличествоКт КАК Количество,
	Рег.Сумма КАК Себестоимость,
	ВЫБОР
		КОГДА Цены.Цена ЕСТЬ NULL 
				ИЛИ Цены.Цена = 0
			ТОГДА ВЫБОР
					КОГДА ЦеныПоДоговору.Цена ЕСТЬ NULL 
							ИЛИ ЦеныПоДоговору.Цена = 0
						ТОГДА 0
					ИНАЧЕ ЦеныПоДоговору.Цена
				КОНЕЦ
		ИНАЧЕ Цены.Цена
	КОНЕЦ КАК Цена,
	ВЫБОР
		КОГДА Цены.Цена ЕСТЬ NULL 
				ИЛИ Цены.Цена = 0
			ТОГДА ВЫБОР
					КОГДА ЦеныПоДоговору.Цена ЕСТЬ NULL 
							ИЛИ ЦеныПоДоговору.Цена = 0
						ТОГДА 0
					ИНАЧЕ ВЫРАЗИТЬ(Рег.КоличествоКт * ЦеныПоДоговору.Цена КАК ЧИСЛО(15, 2))
				КОНЕЦ
		ИНАЧЕ ВЫРАЗИТЬ(Рег.КоличествоКт * Цены.Цена КАК ЧИСЛО(15, 2))
	КОНЕЦ КАК Сумма
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ДвиженияССубконто(
			,
			,
			СчетКт = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ТоварыНаСкладе)
				И СчетДт В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.СебестоимостьРеализации)),
			,
			) КАК Рег
		ЛЕВОЕ СОЕДИНЕНИЕ Цены КАК Цены
		ПО Рег.Регистратор = Цены.Регистратор
			И Рег.СубконтоДт2 = Цены.ДоговорКонтрагента
			И Рег.СубконтоКт1 = Цены.Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ ЦеныПоДоговору КАК ЦеныПоДоговору
		ПО Рег.Регистратор = ЦеныПоДоговору.Регистратор
			И Рег.СубконтоДт2 = ЦеныПоДоговору.ДоговорКонтрагента</query>
		</dataSet>
		<totalField>
			<dataPath>Количество</dataPath>
			<expression>Сумма(Количество)</expression>
		</totalField>
		<totalField>
			<dataPath>Себестоимость</dataPath>
			<expression>Сумма(Себестоимость)</expression>
		</totalField>
		<totalField>
			<dataPath>Сумма</dataPath>
			<expression>Сумма(Сумма)</expression>
		</totalField>
		<parameter>
			<name>НачалоПериода</name>
			<title xmlns:d4p1="http://v8.1c.ru/8.1/data/core" xsi:type="d4p1:LocalStringType">
				<d4p1:item>
					<d4p1:lang>ru</d4p1:lang>
					<d4p1:content>Начало периода</d4p1:content>
				</d4p1:item>
			</title>
			<valueType>
				<Type xmlns="http://v8.1c.ru/8.1/data/core">xs:dateTime</Type>
				<DateQualifiers xmlns="http://v8.1c.ru/8.1/data/core">
					<DateFractions>DateTime</DateFractions>
				</DateQualifiers>
			</valueType>
			<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
			<useRestriction>false</useRestriction>
		</parameter>
		<parameter>
			<name>КонецПериода</name>
			<title xmlns:d4p1="http://v8.1c.ru/8.1/data/core" xsi:type="d4p1:LocalStringType">
				<d4p1:item>
					<d4p1:lang>ru</d4p1:lang>
					<d4p1:content>Конец периода</d4p1:content>
				</d4p1:item>
			</title>
			<valueType>
				<Type xmlns="http://v8.1c.ru/8.1/data/core">xs:dateTime</Type>
				<DateQualifiers xmlns="http://v8.1c.ru/8.1/data/core">
					<DateFractions>DateTime</DateFractions>
				</DateQualifiers>
			</valueType>
			<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
			<useRestriction>false</useRestriction>
