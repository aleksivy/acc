			<query>ВЫБРАТЬ
	Рег.Субконто1 КАК Контрагент,
	Рег.Субконто2 КАК Договор,
	Рег.Субконто3 КАК Приход,
	Рег.СуммаОстатокКт КАК СуммаЗадолженности
ПОМЕСТИТЬ Задолженности
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Остатки(
			КОНЕЦПЕРИОДА(&amp;НаДату, ДЕНЬ),
			Счет = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.РасчетыСОтечественнымиПоставщиками),
			,
			Организация = &amp;Серна
				И Субконто2.ТипДоговора = ЗНАЧЕНИЕ(Перечисление.ТипДоговора.Товары)
				И ТИПЗНАЧЕНИЯ(Субконто3) = ТИП(Документ.ПоступлениеТоваровУслуг)) КАК Рег
ГДЕ
	Рег.СуммаОстатокКт &gt; 1
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Рег.Объект КАК Договор,
	ВЫРАЗИТЬ(Рег.Значение КАК ЧИСЛО(12, 6)) КАК Курс
ПОМЕСТИТЬ КурсыПоДоговорам
ИЗ
	РегистрСведений.ЗначенияСвойствОбъектов КАК Рег
ГДЕ
	Рег.Объект В
			(ВЫБРАТЬ
				Задолженности.Договор
			ИЗ
				Задолженности КАК Задолженности)
	И Рег.Свойство = &amp;СвойствоКурс
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДокТЧ.Ссылка КАК Приход,
	МАКСИМУМ(ДокТЧ.Номенклатура) КАК Номенклатура,
	СУММА(ДокТЧ.СуммаНДС) КАК СуммаНДС,
	СУММА(ВЫБОР
			КОГДА ДокТЧ.Ссылка.СуммаВключаетНДС
				ТОГДА ДокТЧ.Сумма
			ИНАЧЕ ДокТЧ.Сумма + ДокТЧ.СуммаНДС
		КОНЕЦ) КАК СуммаСНДС
ПОМЕСТИТЬ Приходы
ИЗ
	Документ.ПоступлениеТоваровУслуг.Товары КАК ДокТЧ
ГДЕ
	ДокТЧ.Ссылка В
			(ВЫБРАТЬ
				Задолженности.Приход
			ИЗ
				Задолженности КАК Задолженности)

СГРУППИРОВАТЬ ПО
	ДокТЧ.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Задолженности.Контрагент,
	Задолженности.Договор,
	Задолженности.Договор.Дата КАК ДатаДоговора,
	ЕСТЬNULL(КурсыПоДоговорам.Курс, 0) КАК КурсПоДоговору,
	ЕСТЬNULL(ггуПодразделения.Ссылка, ЗНАЧЕНИЕ(Справочник.ггуПодразделения.ПустаяСсылка)) КАК Подразделение,
	ЕСТЬNULL(Приходы.Номенклатура.НоменклатурнаяГруппа, ЗНАЧЕНИЕ(Справочник.НоменклатурныеГруппы.ПустаяСсылка)) КАК НоменклатурнаяГруппа,
	ЕСТЬNULL(Приходы.Номенклатура, ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)) КАК Номенклатура,
	Задолженности.Приход КАК Партия,
	Задолженности.Приход.Дата КАК ДатаПрихода,
	Задолженности.СуммаЗадолженности КАК СуммаЗадолженностиВсего,
	ЕСТЬNULL(Приходы.СуммаСНДС, 0) КАК СуммаСНДСПоПриходу,
