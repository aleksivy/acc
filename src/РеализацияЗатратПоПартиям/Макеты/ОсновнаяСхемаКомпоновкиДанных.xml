ВЫБРАТЬ
	Рег.Регистратор,
	Рег.Склад,
	Рег.Номенклатура,
	Рег.СтатьяЗатрат,
	Рег.СкладНачальный,
	Рег.Содержание,
	Рег.Intercompany,
	Рег.Партия,
	Рег.НалоговоеНазначение,
	Остаток39.НалоговоеНазначение КАК НалоговоеНазначение2015,
	Рег.КоличествоРасход КАК КоличествоПродано,
	Рег.СуммаРасход КАК СуммаПродано,
	Рег.ВалютнаяСуммаРасход КАК ВалютнаяСуммаПродано,
	Остаток39.КоличествоОстаток,
	Остаток39.СуммаОстаток,
	Остаток39.ВалютнаяСуммаОстаток
ИЗ
	РегистрНакопления.СернаЗатратыБудущихПериодов.Обороты(НАЧАЛОПЕРИОДА(&amp;ДатаОтчета, МЕСЯЦ), КОНЕЦПЕРИОДА(&amp;ДатаОтчета, МЕСЯЦ), Регистратор, ) КАК Рег
		ЛЕВОЕ СОЕДИНЕНИЕ Остаток39 КАК Остаток39
		ПО Рег.Номенклатура = Остаток39.Номенклатура
			И Рег.СтатьяЗатрат = Остаток39.СтатьяЗатрат
			И Рег.СкладНачальный = Остаток39.СкладНачальный
			И Рег.Партия = Остаток39.Партия
			И Рег.Содержание = Остаток39.Содержание
			И Рег.Intercompany = Остаток39.Intercompany
ГДЕ
	Рег.Регистратор.СчетДт.Код = "93"
	И Рег.Регистратор.СчетКт.Код = "39"</query>
