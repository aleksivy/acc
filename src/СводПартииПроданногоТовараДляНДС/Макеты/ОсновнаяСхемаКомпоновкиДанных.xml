			<field xsi:type="DataSetFieldField">
				<dataPath>СчетПродажи</dataPath>
				<field>СчетПродажи</field>
				<title xmlns:d5p1="http://v8.1c.ru/8.1/data/core" xsi:type="d5p1:LocalStringType">
					<d5p1:item>
						<d5p1:lang>ru</d5p1:lang>
						<d5p1:content>Счет продажи</d5p1:content>
					</d5p1:item>
				</title>
				<role>
					<dimension xmlns="http://v8.1c.ru/8.1/data-composition-system/common">true</dimension>
				</role>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>СчетТовара</dataPath>
				<field>СчетТовара</field>
				<title xmlns:d5p1="http://v8.1c.ru/8.1/data/core" xsi:type="d5p1:LocalStringType">
					<d5p1:item>
						<d5p1:lang>ru</d5p1:lang>
						<d5p1:content>Счет товара</d5p1:content>
					</d5p1:item>
				</title>
				<role>
					<dimension xmlns="http://v8.1c.ru/8.1/data-composition-system/common">true</dimension>
				</role>
			</field>
			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	Продажи.СчетКт КАК СчетПродажи,
	Продажи.Регистратор,
	Продажи.СубконтоКт2 КАК ДоговорКонтрагента,
	СУММА(Продажи.Сумма) КАК Сумма
ПОМЕСТИТЬ Продажи
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ДвиженияССубконто(
			,
			,
			СчетКт В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ДоходыОтРеализации))
				И СчетДт В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.РасчетыСПокупателямиИЗаказчиками)),
			,
			) КАК Продажи

СГРУППИРОВАТЬ ПО
	Продажи.СчетКт,
	Продажи.Регистратор,
	Продажи.СубконтоКт2
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Партии.Регистратор,
	Партии.СчетКт КАК СчетТовара,
	Партии.СубконтоДт2 КАК ДоговорКонтрагента,
	Партии.СубконтоКт3 КАК Партия,
	Партии.СубконтоКт1 КАК Номенклатура,
	Партии.СубконтоКт1.ОсвобожденоОтНДС КАК ОсвобожденоОтНДС,
	ВЫБОР
		КОГДА ТИПЗНАЧЕНИЯ(Партии.СубконтоКт3) = ТИП(Документ.ОприходованиеТоваров)
			ТОГДА ЗНАЧЕНИЕ(Перечисление.ВидПосредника.ВторойПосредник)
		КОГДА Партии.СубконтоКт3 = Партии.Регистратор
			ТОГДА ЗНАЧЕНИЕ(Перечисление.ВидПосредника.ВторойПосредник)
		ИНАЧЕ Партии.СубконтоКт3.ДоговорКонтрагента.ВидПосредника
	КОНЕЦ КАК ВидПосредника,
	Партии.СубконтоКт3.ДоговорКонтрагента КАК ДоговорПартии,
	Партии.СубконтоКт3.Дата КАК ДатаОприходования,
	Партии.КоличествоКт КАК Количество
ПОМЕСТИТЬ Партии
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ДвиженияССубконто(
			,
			,
			(СчетКт = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ТоварыНаСкладе)
				ИЛИ СчетКт = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ГотоваяПродукция))
				И СчетДт В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.СебестоимостьРеализации)),
			,
			) КАК Партии
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Партии.СчетТовара,
	Партии.Регистратор,
	Партии.Номенклатура,
	Партии.ДоговорКонтрагента,
	Партии.ОсвобожденоОтНДС,
	Партии.ВидПосредника,
	СУММА(Партии.Количество) КАК Количество
ПОМЕСТИТЬ ПартииСвернуто
ИЗ
	Партии КАК Партии

СГРУППИРОВАТЬ ПО
	Партии.СчетТовара,
	Партии.Регистратор,
	Партии.Номенклатура,
	Партии.ДоговорКонтрагента,
	Партии.ОсвобожденоОтНДС,
	Партии.ВидПосредника
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПартииСвернуто.СчетТовара,
	ПартииСвернуто.Регистратор,
	ПартииСвернуто.ДоговорКонтрагента,
	СУММА(ПартииСвернуто.Количество) КАК Количество
ПОМЕСТИТЬ ПартииСвернутоВсе
ИЗ
	ПартииСвернуто КАК ПартииСвернуто

СГРУППИРОВАТЬ ПО
	ПартииСвернуто.СчетТовара,
	ПартииСвернуто.Регистратор,
	ПартииСвернуто.ДоговорКонтрагента
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Продажи.СчетПродажи,
	ЕСТЬNULL(Продажи.Регистратор, ПартииСвернутоВсе.Регистратор) КАК Регистратор,
	ЕСТЬNULL(Продажи.ДоговорКонтрагента, ПартииСвернутоВсе.ДоговорКонтрагента) КАК ДоговорКонтрагента,
	ЕСТЬNULL(ПартииСвернутоВсе.Количество, 0) КАК Количество,
	ЕСТЬNULL(Продажи.Сумма, 0) КАК Сумма,
	ВЫБОР
		КОГДА ЕСТЬNULL(ПартииСвернутоВсе.Количество, 0) = 0
			ТОГДА 0
		ИНАЧЕ ЕСТЬNULL(Продажи.Сумма, 0) / ЕСТЬNULL(ПартииСвернутоВсе.Количество, 0)
	КОНЕЦ КАК Цена
ПОМЕСТИТЬ Цены
ИЗ
	Продажи КАК Продажи
		ПОЛНОЕ СОЕДИНЕНИЕ ПартииСвернутоВсе КАК ПартииСвернутоВсе
		ПО Продажи.Регистратор = ПартииСвернутоВсе.Регистратор
			И Продажи.ДоговорКонтрагента = ПартииСвернутоВсе.ДоговорКонтрагента
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПартииСвернуто.СчетТовара КАК СчетТовара,
	ПартииСвернуто.Регистратор,
	ПартииСвернуто.Номенклатура КАК Номенклатура,
	ПартииСвернуто.ДоговорКонтрагента,
	ПартииСвернуто.ОсвобожденоОтНДС,
	ПартииСвернуто.ВидПосредника КАК ВидПосредника,
	ПартииСвернуто.Количество,
	Цены.СчетПродажи,
	Цены.Цена,
	ВЫБОР
		КОГДА ПартииСвернуто.ОсвобожденоОтНДС
			ТОГДА ВЫБОР
					КОГДА ПартииСвернуто.ВидПосредника = ЗНАЧЕНИЕ(Перечисление.ВидПосредника.Производитель)
						ТОГДА ВЫБОР
								КОГДА Цены.СчетПродажи = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ДоходОтРеализацииРаботИУслуг)
									ТОГДА ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС0)
								ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС20)
							КОНЕЦ
					ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.БезНДС)
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА Цены.СчетПродажи = ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ДоходОтРеализацииРаботИУслуг)
					ТОГДА ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС0)
				ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС20)
			КОНЕЦ
	КОНЕЦ КАК СтавкаНДС,
	ВЫРАЗИТЬ(ПартииСвернуто.Количество * Цены.Цена КАК ЧИСЛО(12, 2)) КАК Сумма
ПОМЕСТИТЬ Врем
ИЗ
	ПартииСвернуто КАК ПартииСвернуто
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Цены КАК Цены
		ПО ПартииСвернуто.Регистратор = Цены.Регистратор
			И ПартииСвернуто.ДоговорКонтрагента = Цены.ДоговорКонтрагента
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Врем.СчетТовара,
	Врем.Регистратор,
	Врем.Номенклатура,
	Врем.ДоговорКонтрагента,
	Врем.ОсвобожденоОтНДС,
	Врем.ВидПосредника,
	Врем.Количество,
	Врем.СчетПродажи,
	Врем.Цена,
	Врем.СтавкаНДС,
	ВЫБОР
