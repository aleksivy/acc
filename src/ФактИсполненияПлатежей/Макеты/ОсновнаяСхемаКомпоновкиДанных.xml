			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	НАЧАЛОПЕРИОДА(Док.Дата, ДЕНЬ) КАК Дата,
	ВЫБОР
		КОГДА СУММА(Док.СуммаВалютная) &gt; 0
			ТОГДА ВЫРАЗИТЬ(СУММА(Док.СуммаГривневая) / СУММА(Док.СуммаВалютная) КАК ЧИСЛО(12, 6))
		ИНАЧЕ 0
	КОНЕЦ КАК КурсБанка,
	ВЫБОР
		КОГДА СУММА(Док.СуммаВалютная) &gt; 0
			ТОГДА ВЫРАЗИТЬ(СУММА(Док.СуммаГривневая - Док.СуммаКомиссионные) / СУММА(Док.СуммаВалютная) КАК ЧИСЛО(12, 6))
		ИНАЧЕ 0
	КОНЕЦ КАК КурсСУчетомКомиссии
ПОМЕСТИТЬ КурсыВсе
ИЗ
	Документ.ПокупкаПродажаВалюты КАК Док
ГДЕ
	Док.Проведен
	И Док.Дата &gt;= ДАТАВРЕМЯ(2018, 1, 1)
	И Док.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПокупкаПродажаВалюты.ПродажаВалюты)

СГРУППИРОВАТЬ ПО
	НАЧАЛОПЕРИОДА(Док.Дата, ДЕНЬ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	КурсыВсе.Дата КАК ДатаНач,
	МИНИМУМ(ЕСТЬNULL(КурсыВсе1.Дата, ДАТАВРЕМЯ(2100, 1, 1))) КАК ДатаКон,
	КурсыВсе.КурсБанка,
	КурсыВсе.КурсСУчетомКомиссии
ПОМЕСТИТЬ КурсыПоПериодам
ИЗ
	КурсыВсе КАК КурсыВсе
		ЛЕВОЕ СОЕДИНЕНИЕ КурсыВсе КАК КурсыВсе1
		ПО КурсыВсе.Дата &lt; КурсыВсе1.Дата

СГРУППИРОВАТЬ ПО
	КурсыВсе.Дата,
	КурсыВсе.КурсБанка,
	КурсыВсе.КурсСУчетомКомиссии

ИНДЕКСИРОВАТЬ ПО
	ДатаНач
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Рег.Период КАК Дата,
	ВЫБОР
		КОГДА Рег.Кратность = 0
				ИЛИ Рег.Кратность = 1
			ТОГДА Рег.Курс
		ИНАЧЕ Рег.Курс / Рег.Кратность
	КОНЕЦ КАК Курс
ПОМЕСТИТЬ КурсыНБУВсе
ИЗ
	РегистрСведений.КурсыВалют КАК Рег
ГДЕ
	Рег.Период &gt;= ДАТАВРЕМЯ(2018, 1, 1)
	И Рег.Валюта = ЗНАЧЕНИЕ(Справочник.Валюты.Доллар)
	И Рег.ВалютаПриведения = ЗНАЧЕНИЕ(Справочник.Валюты.Гривна)

ИНДЕКСИРОВАТЬ ПО
	Дата
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	КурсыВсе.Дата КАК ДатаНач,
	МИНИМУМ(ЕСТЬNULL(КурсыВсе1.Дата, ДАТАВРЕМЯ(2100, 1, 1))) КАК ДатаКон,
	КурсыВсе.Курс
ПОМЕСТИТЬ КурсыНБУПоПериодам
ИЗ
	КурсыНБУВсе КАК КурсыВсе
		ЛЕВОЕ СОЕДИНЕНИЕ КурсыНБУВсе КАК КурсыВсе1
		ПО КурсыВсе.Дата &lt; КурсыВсе1.Дата

СГРУППИРОВАТЬ ПО
	КурсыВсе.Дата,
	КурсыВсе.Курс

ИНДЕКСИРОВАТЬ ПО
	ДатаНач
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Рег.Регистратор КАК Платежка,
	МАКСИМУМ(КурсыПоПериодам.КурсСУчетомКомиссии) КАК КурсСУчетомКомиссии,
	СУММА(ВЫБОР
			КОГДА Рег.ПродажаВалюты = ЗНАЧЕНИЕ(Документ.ПокупкаПродажаВалюты.ПустаяСсылка)
				ТОГДА ВЫБОР
						КОГДА КурсыПоПериодам.КурсСУчетомКомиссии ЕСТЬ NULL
								ИЛИ КурсыПоПериодам.КурсСУчетомКомиссии = 0
							ТОГДА 0
						ИНАЧЕ ВЫРАЗИТЬ(Рег.СуммаРасход / КурсыПоПериодам.КурсСУчетомКомиссии КАК ЧИСЛО(12, 2))
					КОНЕЦ
			ИНАЧЕ ВЫБОР
					КОГДА Рег.ПродажаВалюты.КурсСУчетомКомиссии = 0
						ТОГДА 0
					ИНАЧЕ ВЫРАЗИТЬ(100 * Рег.СуммаРасход / Рег.ПродажаВалюты.КурсСУчетомКомиссии КАК ЧИСЛО(12, 2))
				КОНЕЦ
		КОНЕЦ) КАК СуммаДолДляКурса,
	СУММА(ВЫБОР
			КОГДА Рег.ПродажаВалюты = ЗНАЧЕНИЕ(Документ.ПокупкаПродажаВалюты.ПустаяСсылка)
				ТОГДА ВЫБОР
						КОГДА КурсыПоПериодам.КурсСУчетомКомиссии ЕСТЬ NULL
								ИЛИ КурсыПоПериодам.КурсСУчетомКомиссии = 0
							ТОГДА 0
						ИНАЧЕ Рег.СуммаРасход
					КОНЕЦ
			ИНАЧЕ ВЫБОР
					КОГДА Рег.ПродажаВалюты.КурсСУчетомКомиссии = 0
						ТОГДА 0
					ИНАЧЕ Рег.СуммаРасход
				КОНЕЦ
		КОНЕЦ) КАК СуммаГрнДляКурса
ПОМЕСТИТЬ КурсыПоПлатежам
ИЗ
	РегистрНакопления.ггуИсточникиФинансирования.Обороты(НАЧАЛОПЕРИОДА(&amp;ДатаНач, ДЕНЬ), КОНЕЦПЕРИОДА(&amp;ДатаКон, ДЕНЬ), Регистратор, Валюта = ЗНАЧЕНИЕ(Справочник.Валюты.Доллар)) КАК Рег
		ЛЕВОЕ СОЕДИНЕНИЕ КурсыПоПериодам КАК КурсыПоПериодам
		ПО Рег.ДатаФинансирования &gt;= КурсыПоПериодам.ДатаНач
			И Рег.ДатаФинансирования &lt; КурсыПоПериодам.ДатаКон
ГДЕ
	ТИПЗНАЧЕНИЯ(Рег.Регистратор) = ТИП(Документ.ПлатежноеПоручениеИсходящее)

СГРУППИРОВАТЬ ПО
	Рег.Регистратор
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДокТЧ.ДоговорКонтрагента.ТипДоговора КАК ТипДоговора,
	НАЧАЛОПЕРИОДА(ДокТЧ.Ссылка.Дата, ДЕНЬ) КАК Период,
	ВЫБОР
		КОГДА ДокТЧ.ДоговорКонтрагента.ПривязкаКВалюте
			ТОГДА ДокТЧ.Ссылка.Дата
		ИНАЧЕ ВЫБОР
				КОГДА ДокТЧ.Ссылка.ЗаказПоставщикуИзУПП.Дата ЕСТЬ NULL
					ТОГДА ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(ДокТЧ.Ссылка.Дата, ДЕНЬ), ДЕНЬ, -1)
				ИНАЧЕ НАЧАЛОПЕРИОДА(ДокТЧ.Ссылка.ЗаказПоставщикуИзУПП.Дата, ДЕНЬ)
			КОНЕЦ
	КОНЕЦ КАК ПериодДляКурса,
	ВЫБОР
		КОГДА ПОДСТРОКА(ДокТЧ.ДоговорКонтрагента.Номер, 1, 2) = "OP"
			ТОГДА "Одеса"
		КОГДА ПОДСТРОКА(ДокТЧ.ДоговорКонтрагента.Номер, 1, 2) = "VP"
			ТОГДА "Винница"
		КОГДА ПОДСТРОКА(ДокТЧ.ДоговорКонтрагента.Номер, 1, 2) = "MP"
			ТОГДА "Харьков"
		КОГДА ПОДСТРОКА(ДокТЧ.ДоговорКонтрагента.Номер, 1, 2) = "CP"
			ТОГДА "Чернигов"
		ИНАЧЕ "Киев"
	КОНЕЦ КАК Подразделение,
	ДокТЧ.ДоговорКонтрагента.Номенклатура КАК Номенклатура,
	ДокТЧ.ДоговорКонтрагента.Номенклатура.НоменклатурнаяГруппа КАК НоменклатурнаяГруппа,
	ДокТЧ.ДоговорКонтрагента.Владелец КАК Контрагент,
	ДокТЧ.ДоговорКонтрагента КАК Договор,
	ДокТЧ.ДоговорКонтрагента.ГрафикОплаты КАК ГрафикОплаты,
	ДокТЧ.Ссылка.ЗаказПоставщикуИзУПП.ЭтапОплаты КАК ЭтапОплаты,
	ДокТЧ.Ссылка КАК Платеж,
	ДокТЧ.Ссылка.ЗаказПоставщикуИзУПП КАК Заказ,
	ДокТЧ.СуммаПлатежа КАК Сумма,
	ВЫБОР
		КОГДА ДокТЧ.ДоговорКонтрагента.ГрафикОплаты = &amp;_80ПоПОставке
			ТОГДА ВЫБОР
					КОГДА ДокТЧ.Ссылка.ЗаказПоставщикуИзУПП.ЭтапОплаты = ЗНАЧЕНИЕ(Перечисление.ггуЭтапыОплаты.ПоПоставке)
						ТОГДА 1
					КОГДА ДокТЧ.Ссылка.ЗаказПоставщикуИзУПП.ЭтапОплаты = ЗНАЧЕНИЕ(Перечисление.ггуЭтапыОплаты.Баланс)
						ТОГДА 1 / 6
					ИНАЧЕ 5 / 6
				КОНЕЦ
		ИНАЧЕ 5 / 6
	КОНЕЦ КАК КоэффициентОплатыТовара,
	ВЫБОР
		КОГДА ДокТЧ.ДоговорКонтрагента.ГрафикОплаты = &amp;_80ПоПОставке
			ТОГДА ВЫБОР
					КОГДА ДокТЧ.Ссылка.ЗаказПоставщикуИзУПП.ЭтапОплаты = ЗНАЧЕНИЕ(Перечисление.ггуЭтапыОплаты.ПоПоставке)
						ТОГДА 0
					КОГДА ДокТЧ.Ссылка.ЗаказПоставщикуИзУПП.ЭтапОплаты = ЗНАЧЕНИЕ(Перечисление.ггуЭтапыОплаты.Баланс)
						ТОГДА 5 / 6
					ИНАЧЕ 1 / 6
				КОНЕЦ
		ИНАЧЕ 1 / 6
	КОНЕЦ КАК КоэффициентОплатыНДС,
	ДокТЧ.СуммаПлатежа * ВЫБОР
		КОГДА ДокТЧ.ДоговорКонтрагента.ГрафикОплаты = &amp;_80ПоПОставке
			ТОГДА ВЫБОР
					КОГДА ДокТЧ.Ссылка.ЗаказПоставщикуИзУПП.ЭтапОплаты = ЗНАЧЕНИЕ(Перечисление.ггуЭтапыОплаты.ПоПоставке)
						ТОГДА 1
					КОГДА ДокТЧ.Ссылка.ЗаказПоставщикуИзУПП.ЭтапОплаты = ЗНАЧЕНИЕ(Перечисление.ггуЭтапыОплаты.Баланс)
						ТОГДА 1 / 6
					ИНАЧЕ 5 / 6
				КОНЕЦ
		ИНАЧЕ 5 / 6
	КОНЕЦ КАК СуммаОплатыТовара,
	ДокТЧ.СуммаПлатежа * ВЫБОР
		КОГДА ДокТЧ.ДоговорКонтрагента.ГрафикОплаты = &amp;_80ПоПОставке
			ТОГДА ВЫБОР
					КОГДА ДокТЧ.Ссылка.ЗаказПоставщикуИзУПП.ЭтапОплаты = ЗНАЧЕНИЕ(Перечисление.ггуЭтапыОплаты.ПоПоставке)
						ТОГДА 0
					КОГДА ДокТЧ.Ссылка.ЗаказПоставщикуИзУПП.ЭтапОплаты = ЗНАЧЕНИЕ(Перечисление.ггуЭтапыОплаты.Баланс)
						ТОГДА 5 / 6
					ИНАЧЕ 1 / 6
				КОНЕЦ
		ИНАЧЕ 1 / 6
	КОНЕЦ КАК СуммаОплатыНДС,
	ВЫРАЗИТЬ(ЕСТЬNULL(КурсыПоПлатежам.СуммаДолДляКурса, 0) КАК ЧИСЛО(12, 2)) КАК СуммаДолДляКурса,
	ВЫРАЗИТЬ(ЕСТЬNULL(КурсыПоПлатежам.СуммаГрнДляКурса, 0) КАК ЧИСЛО(12, 2)) КАК СуммаГрнДляКурса
ПОМЕСТИТЬ Врем1
ИЗ
	Документ.ПлатежноеПоручениеИсходящее.РасшифровкаПлатежа КАК ДокТЧ
		ЛЕВОЕ СОЕДИНЕНИЕ КурсыПоПлатежам КАК КурсыПоПлатежам
		ПО ДокТЧ.Ссылка = КурсыПоПлатежам.Платежка
ГДЕ
	ДокТЧ.ДоговорКонтрагента.ТипДоговора = ЗНАЧЕНИЕ(Перечисление.ТипДоговора.Товары)
	И НЕ ДокТЧ.Ссылка.ПометкаУдаления
	И ДокТЧ.Ссылка.ОтправленоВБанк
	И ДокТЧ.Ссылка.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийППИсходящее.ОплатаПоставщику)
	И ДокТЧ.Ссылка.Дата &gt;= НАЧАЛОПЕРИОДА(&amp;ДатаНач, ДЕНЬ)
	И ДокТЧ.Ссылка.Дата &lt;= КОНЕЦПЕРИОДА(&amp;ДатаКон, ДЕНЬ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Врем.ТипДоговора,
	Врем.Период,
	Врем.Подразделение,
	Врем.Номенклатура,
	Врем.НоменклатурнаяГруппа,
	Врем.Контрагент,
	Врем.Договор,
	Врем.ГрафикОплаты,
	Врем.ЭтапОплаты,
	Врем.Платеж,
	Врем.Заказ,
	Врем.Сумма,
	Врем.КоэффициентОплатыТовара,
	Врем.КоэффициентОплатыНДС,
	Врем.СуммаОплатыТовара,
	Врем.СуммаОплатыНДС,
	Врем.СуммаДолДляКурса,
	Врем.СуммаГрнДляКурса,
	ВЫБОР
		КОГДА Врем.Договор.КурсПоДоговору = 0
			ТОГДА 0
		ИНАЧЕ Врем.СуммаОплатыТовара
	КОНЕЦ КАК СуммаГрнДляКурсаПоДоговору,
	ВЫБОР
		КОГДА Врем.Договор.ПривязкаКВалюте
			ТОГДА ВЫБОР
					КОГДА ЕСТЬNULL(КурсыНБУПоПериодам.Курс, 0) = 0
						ТОГДА 0
					ИНАЧЕ Врем.СуммаОплатыТовара / ЕСТЬNULL(КурсыНБУПоПериодам.Курс, 0)
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА Врем.Договор.КурсПоДоговору = 0
					ТОГДА 0
				ИНАЧЕ Врем.СуммаОплатыТовара / Врем.Договор.КурсПоДоговору
			КОНЕЦ
	КОНЕЦ КАК СуммаДолДляКурсаПоДоговору,
	ВЫБОР
		КОГДА Врем.Договор.ПривязкаКВалюте
			ТОГДА ЕСТЬNULL(КурсыНБУПоПериодам.Курс, 0)
		ИНАЧЕ Врем.Договор.КурсПоДоговору
	КОНЕЦ КАК КурсПоДоговору
ПОМЕСТИТЬ Врем2
ИЗ
	Врем1 КАК Врем
		ЛЕВОЕ СОЕДИНЕНИЕ КурсыНБУПоПериодам КАК КурсыНБУПоПериодам
		ПО Врем.ПериодДляКурса &gt;= КурсыНБУПоПериодам.ДатаНач
			И Врем.ПериодДляКурса &lt; КурсыНБУПоПериодам.ДатаКон
;

