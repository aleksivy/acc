	ЕСТЬNULL(РегОС.СуммаНУОборотКт, 0) КАК СуммаНУОборотКтОС,
	ЕСТЬNULL(РегОС.СуммаНУКонечныйОстатокДт, 0) КАК СуммаНУКонечныйОстатокОС,
	ЕСТЬNULL(РегИзнос.СуммаНУНачальныйОстатокКт, 0) КАК СуммаНУНачальныйОстатокИзнос,
	ЕСТЬNULL(РегИзнос.СуммаНУОборотДт, 0) КАК СуммаНУОборотДтИзнос,
	ЕСТЬNULL(РегИзнос.СуммаНУОборотКт, 0) КАК СуммаНУОборотКтИзнос,
	ЕСТЬNULL(РегИзнос.СуммаНУОборотКт, 0) КАК СуммаНУНачисленныйИзнос,
